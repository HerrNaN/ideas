%format :||:  = "\mathbin{\vee}"
%format :&&:  = "\mathbin{\wedge}"
%format :->:  = "\mathbin{\rightarrow}"
%format :<->: = "\mathbin{\leftrightarrow}"
%format Not   = "\neg"
%format Var (a) = a
%format T     = "T"
%format F     = "F"
%format _0    = "\phi"
%format _1    = "\psi"
%format _2    = "\chi"
%format _3    = "\tau"

%new directives for equivalence
%format :~>  = "\:\mathbin{\Leftrightarrow}\:"
%format :/~> = "\:\mathbin{\not\Leftrightarrow}\:"