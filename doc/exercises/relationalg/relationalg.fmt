%format :||:  = "\mathbin{\cup}"
%format :&&:  = "\mathbin{\cap}"
%format :+:   = "\mathbin{\dagger}"
%format :.:   = "\mathbin{;}"
%format Not (a) = "\overline{"a"}"
%format Inv a = "{"a"}^{\smile}"
%format Var a = a
%format V     = "V"
%format I     = "I"
%format _0    = "q"
%format _1    = "r"
%format _2    = "s"
%format _3    = "t"