%format :||:  = "\mathbin{\cup}"
%format :&&:  = "\mathbin{\cap}"
%format :+:   = "\mathbin{\dagger}"
%format :.:   = "\mathbin{;}"
%format Not (a) = "\overline{"a"}"
%format Inv a = "{"a"}^{\smile}"
%format Var a = a
%format V     = "V"
%format I     = "I"
%format a     = "{q}"
%format b     = "{r}"
%format c     = "{s}"
%format d     = "{t}"